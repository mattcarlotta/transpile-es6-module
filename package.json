{
	"name": "react-bb-kit",
	"version": "2.0.0",
	"description": "A starter kit for React applications.",
	"main": "./src/index.js",
	"repository": "https://github.com/mattcarlotta/react-bb-kit",
	"author": "Matt Carlotta",
	"license": "MIT",
	"private": false,
	"engines": {
		"node": ">=10",
		"npm": ">=6"
	},
	"scripts": {
		"compile": "rimraf build && babel ./server.js -d build --no-comments",
		"dev": "better-npm-run dev",
		"checkbuild": "es-check  es5 './dist/**/*.js'",
		"initialize": "yarn install",
		"build": "better-npm-run build",
		"start": "better-npm-run start",
		"stage": "better-npm-run stage",
		"analyze": "better-npm-run analyze",
		"test": "better-npm-run test",
		"test:watch": "better-npm-run test:watch",
		"test:watchall": "better-npm-run test:watchall",
		"test:coverage": "better-npm-run test:coverage",
		"format": "prettier --write 'src/**/*.js'",
		"lintjs": "eslint 'src/**/*.js'",
		"lintstyles": "stylelint **/*.scss",
		"pre-commit": "lint-staged"
	},
	"betterScripts": {
		"dev": {
			"command": "webpack-dev-server",
			"env": {
				"APIPORT": 5000,
				"BASEURL": "http://localhost:8080/api/",
				"NODE_ENV": "development",
				"PORT": 3000
			}
		},
		"build": {
			"command": "rimraf dist && webpack",
			"env": {
				"APIPORT": 8080,
				"BASEURL": "https://example.com/api/",
				"NODE_ENV": "production"
			}
		},
		"stage": {
			"command": "rimraf dist && webpack",
			"env": {
				"APIPORT": 8080,
				"BASEURL": "http://localhost:8080/api/",
				"NODE_ENV": "production",
				"NODE_ENV_DEV": "staging"
			}
		},
		"start": {
			"command": "node ./build/server.js",
			"env": {
				"HOST": "http://localhost:",
				"CLIENT": "https://example.com",
				"NODE_ENV": "production",
				"PORT": 8080
			}
		},
		"analyze": {
			"command": "rimraf dist && webpack",
			"env": {
				"APIPORT": 8080,
				"BASEURL": "http://localhost:8080/api/",
				"NODE_ENV": "production",
				"NODE_ENV_DEV": "staging",
				"ANALZYER": true
			}
		},
		"test": {
			"command": "jest --config ./jest.json",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:coverage": {
			"command": "jest --config ./jest.json --coverage",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:watch": {
			"command": "jest --config ./jest.json --watch --onlyChanged",
			"env": {
				"NODE_ENV": "testing"
			}
		},
		"test:watchall": {
			"command": "jest --config ./jest.json --watchAll",
			"env": {
				"NODE_ENV": "testing"
			}
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"git add"
		],
		"*.scss": [
			"stylelint **/*.scss",
			"git add"
		],
		"*.{json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"dependencies": {
		"@atlaskit/tooltip": "^15.2.5",
		"axios": "^0.19.2",
		"better-npm-run": "^0.1.1",
		"express": "^4.17.1",
		"lodash": "^4.17.15",
		"morgan": "^1.9.1",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.1.2",
		"styled-components": "3.2.6"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"axios-mock-adapter": "^1.17.0",
		"babel-core": "^7.0.0-beta.4",
		"babel-eslint": "^10.0.3",
		"babel-jest": "^25.1.0",
		"babel-loader": "^8.0.6",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-preset-env": "^1.7.0",
		"better-npm-run": "^0.1.1",
		"browserslist": "^4.8.6",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.2",
		"enzyme": "^3.10.0",
		"enzyme-adapter-react-16": "^1.15.1",
		"error-overlay-webpack-plugin": "^0.4.1",
		"es-check": "^5.1.0",
		"eslint": "^6.7.2",
		"eslint-config-airbnb": "^18.0.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-jest": "^23.7.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^2.3.0",
		"eslint-watch": "^6.0.1",
		"file-loader": "^5.0.2",
		"friendly-errors-webpack-plugin": "^1.7.0",
		"html-webpack-plugin": "^4.0.0-beta.11",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^25.1.0",
		"jest-css-modules-transform": "^4.0.0",
		"jest-enzyme": "^7.1.2",
		"jest-haste-map": "^25.1.0",
		"jest-resolve": "^25.1.0",
		"jsdom": "^15.2.1",
		"lint-staged": "^10.0.7",
		"mini-css-extract-plugin": "^0.9.0",
		"node-sass": "^4.13.1",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.19.1",
		"react-dev-utils": "^10.1.0",
		"rimraf": "^3.0.2",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3",
		"stylelint": "^13.1.0",
		"stylelint-config-recommended": "^3.0.0",
		"stylelint-scss": "^3.14.2",
		"terser-webpack-plugin": "^2.3.4",
		"webpack": "^4.41.6",
		"webpack-bundle-analyzer": "^3.6.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.10.3",
		"webpack-manifest-plugin": "^2.2.0",
		"webpackbar": "^4.0.0"
	}
}
